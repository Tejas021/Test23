<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
        <servlet-name>DocumentServlet</servlet-name>
        <servlet-class>com.example.DocumentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DocumentServlet</servlet-name>
        <url-pattern>/document/all</url-pattern>
        <url-pattern>/document/byemail/*</url-pattern>
        <url-pattern>/document/*</url-pattern>
         
    </servlet-mapping>

    <servlet>
        <servlet-name>GroupServlet</servlet-name>
        <servlet-class>main.java.com.example.Groups.GroupServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GroupServlet</servlet-name>
        <url-pattern>/group/all</url-pattern>
        <url-pattern>/group/*</url-pattern>
    </servlet-mapping>

<servlet>
        <servlet-name>DocumentGroupServlet</servlet-name>
        <servlet-class>main.java.com.example.DocumentGroup.DocumentGroupServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DocumentGroupServlet</servlet-name>
        <url-pattern>/docgroup/</url-pattern>
  
    </servlet-mapping>

    <servlet>
        <servlet-name>GoogleAPIServlet</servlet-name>
        <servlet-class>main.java.com.example.GoogleAPI.GoogleAPIServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>GoogleAPIServlet</servlet-name>
        <url-pattern>/googleapi/</url-pattern>
  
    </servlet-mapping>

</web-app>
